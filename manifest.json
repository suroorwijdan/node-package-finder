{
  "name": "Node Package Finder",
  "version": "0.0.8",
  "manifest_version": 2,
  "minimum_chrome_version": "29",  
  "icons": {
    "128": "icon.png"
  },   
  "permissions": [
    "identity",
    "http://registry.npmjs.org/",    
    "https://www.npmjs.org/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  }

}